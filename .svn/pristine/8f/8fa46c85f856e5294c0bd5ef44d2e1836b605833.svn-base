   /* 中等尺寸手机 */
   @media screen and (max-height:430px)  {
        
      
    #app {
      display: none;
      width: 100vw;
      height: 100vh;
      position: absolute;
    }

    #loadings {
      position: absolute;
      z-index: -99;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      display: flex;
      justify-content: center;
      align-items: center;
    }

    #update {
      width: 96px;
      height: 96px;
      background: url("https://lz06-1301325388.cos.ap-chengdu.myqcloud.com/storiesinmusic/asset/model/loading.png");
      animation: move 0.9s steps(12, end) infinite;
    }

    #models {
      background: url("../../../CommonAsset/img/界面bg.png") no-repeat;
      background-size: 100% 100%;
      position: absolute;
      z-index: -1;
    }

    .common {
      width: 80%;
      margin: 0 auto;
    }

    .topBox {
      height: 75vh;
    }

    .bottomBox {
      display: flex;
      justify-content: space-around;
      align-items: center;
      height: 60px;
      background: url("../../../CommonAsset/img/按钮底板.png") no-repeat;
      background-size: 100%;
    }

    .dialog {
      position: absolute;
      left: 20%;
      top: 5%;
      width: 270px;
      height: 220px;
      background: url("../../../CommonAsset/img/对话框.png") no-repeat;
      background-size: 100%;
    }

    .renwu {
      position: relative;
      left: 68%;
      top: 20%;
      width: 50px;
      height: 50px;
    }

    .panda {
      position: relative;
      left: 61%;
      top: 23%;
      width: 120px;
      height: 150px;
      opacity: 0.5;
    }

    .mission2 {
      position: relative;
      top: 25%;
      left: 12%;
      width: 25vw;
    }

    .mission3 {
      position: relative;
      top: 30%;
      left: 12%;
      width: 25vw;
      height: 20vh;
    }

    .image {
      width: 50%;
      height: 50%;
    }

    .back {
      position: absolute;
      width: 38px;
      height: 38px;
      left: 92%;
      top: 5%;
      background: url("../../../CommonAsset/img/返回.png") no-repeat;
      background-size: 100%;
    }

    .mission1 {
      position: relative;
      top: 40%;
      left: 12%;
      width: 27vw;
      height: 22vh;
    }

    .mission4 {
      position: relative;
      top: 25%;
      left: 30%;
      width: 20vw;
      height: 30vh;
    }

    .mission5 {
      position: relative;
      top: 22%;
      left: 25%;
      width: 20vw;
      height: 30vh;
    }

    .mission4_video {
      margin-top: 3%;
      font-size: 16px;
      font-weight: bold;
    }

    .mission_title {
      width: 100%;
      margin-top: 5%;
      font-size: 22px;
      font-weight: bold;
      text-align: center;
    }

    .mission_text {
      width: 100%;
      text-align: center;
    }
    .renwu_img{
      width: 50%;
      animation:mymove 5s infinite;
    }
    @keyframes move {
      0% {
        background-position: 0 0;
      }

      100% {
        background-position: -1152px 0;
      }
    }
   .bottomImg{
     width: 54px;
     height: 54px;
     text-align: center;
     line-height: 54px;
   }
   .bottomImg > img {
     width: 80%;
   }
  }
